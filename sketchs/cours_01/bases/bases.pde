// size = Window size
// size(width, height);
size(800, 500);

// background = window background color
// background(red, green, blue);
background(100, 100, 100);

// ellipse = makes a circle in the window
// ellipse(x, y, width, height); the circle's origin point is the center.
fill(170,170,255,67);
ellipse(400f, 250f, 80f, 40f);

// stroke = color of the stroke of the next shape listed. RGB args.
// fill = color of the next shape listed after. RGB arguments.
// square = makes a square in the window
// square(x, y, size); the square's origin point is the top left
stroke(255, 255, 255);
fill(200, 50, 0);
square(375f, 100f, 50f);

// rect = rectangle. rect(x, y, width, height);
fill(255, 255, 0, 100);
rect(350f, 300f, 100f, 150f);
